{
  "extends": ["next/core-web-vitals", "next/typescript",  "plugin:react/recommended", "plugin:prettier/recommended"],
  "plugins": ["prettier"],
  "rules": {
    "arrow-parens": ["error", "as-needed"],
    "react/react-in-jsx-scope": 0,
    "react/prop-types": 0,
    "react/jsx-curly-brace-presence": [
      "error",
      { "props": "never", "children": "never" }
    ]
  }
}
